{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "description": "Backend boilerplate",
  "main": "src/bin/www.js",
  "scripts": {
    "start": "nodemon --exec   \"npm run lint &&  babel-node  ./src/bin/www.js\" --ignore src/util/swagger/*.json ",
    "generateKeyPair": "babel-node src/util/generateKeyPair.js",
    "lint": "eslint ./src --fix",
    "test": "mocha --require @babel/register test/",
    "prebuild": "rm -rf build/",
    "build": "babel src -d build --copy-files",
    "coverage": "nyc --reporter=lcov --reporter=html npm run test",
    "prod": "npm run build && node ./build/bin/www.js"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "bson": "^4.6.0",
    "cache-manager": "^3.6.0",
    "cache-manager-mongoose": "^1.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^6.0.13",
    "morgan": "~1.9.1",
    "mssql": "^7.3.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "swagger-autogen": "^2.14.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.17.7",
    "@types/express": "^4.17.13",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-jsdoc-essential": "^0.1.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  }
}
